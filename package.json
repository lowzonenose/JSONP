{
  "name": "@lowzonenose/jsonp",
  "version": "0.0.2",
  "description": "JSONP (or JSON with Padding) is a technique used by web developers to overcome the cross-domain restrictions imposed by browsers to allow data to be retrieved from systems other than the one the page was served by.",
  "main": "dist/jsonp.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "minify": "gulp --minify",
    "build": "gulp",
    "test": "mocha --ui exports --reporter spec",
    "monitor": "mocha --ui exports --reporter min --watch",
    "debugtest": "mocha --debug-brk --ui exports --reporter spec",
    "coverage": "mochacoverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lowzonenose/JSONP.git"
  },
  "keywords": [
    "jsonp"
  ],
  "author": "lowzonenose",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/lowzonenose/JSONP/issues"
  },
  "homepage": "https://github.com/lowzonenose/JSONP#readme",
  "dependencies": {
    "amdclean": "^2.7.0",
    "chai": "^3.4.1",
    "easy-amdtest": "^0.1.0",
    "groundskeeper": "^0.1.12",
    "gulp": "^3.9.0",
    "gulp-clean": "^0.3.1",
    "gulp-connect": "~1.1.0",
    "gulp-header": "^1.7.1",
    "gulp-load-plugins": "^1.1.0",
    "gulp-mocha-phantomjs": "^0.7.0",
    "gulp-plumber": "^0.5.6",
    "gulp-size": "^0.1.3",
    "gulp-uglify": "^1.4.2",
    "gulp-umd": "^0.2.0",
    "gulp-util": "^3.0.7",
    "minimist": "^1.2.0",
    "mocha": "^2.2.5",
    "requirejs": "^2.1.20",
    "run-sequence": "^1.1.4",
    "shelljs": "^0.5.3",
    "should": "^7.1.1",
    "sinon": "^1.17.2"
  },
  "devDependencies": {
    "coveralls": "^2.11.4"
  }
}
